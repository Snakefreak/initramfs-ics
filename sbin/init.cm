#!/sbin/busybox sh

echo "found Cyanogen System, booting up"

export _PATH="$PATH"
export PATH=/sbin

cd /
busybox date >>boot-log.txt
exec >>boot-log.txt 2>&1

rm /init
mv /innt /init
# busybox cp -a /res/misc/cyano/* /

busybox date >>boot-log.txt
busybox rm -fr /res/misc/cyano /dev/*
export PATH="${_PATH}"
exec /init
